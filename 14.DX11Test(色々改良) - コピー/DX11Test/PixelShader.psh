struct PS_IN
{
    float4 pos : SV_POSITION;
	float2 tex : TEXTURE0;
};

Texture2D    myTexture : register(t0); //テクスチャー
SamplerState mySampler : register(s0); //サンプラー
 
float4 ps_main( PS_IN input ) : SV_Target
{
    float4 tex = myTexture.Sample(mySampler, input.tex);

    if (tex.a <= 0)
    {
        discard;
    }

    return tex;
}